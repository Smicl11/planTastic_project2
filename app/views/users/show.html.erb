

<!-- Profile side bar -->
<div class="container">
  <div class="col-md-4">
    <div class="row">
      <%= image_tag @user.profile_image, class: "img-responsive img-thumbnail"%>
    </div>
    <div class="row">
      <h3>Welcome, <%= @user.first_name %></h3>
    </div>
    <div class="row">
      <table class="table table-striped">
        <tbody>
          <tr>
            <td>First Name:</td>
            <td><%= @user.first_name %></td>
          </tr>
          <tr>
            <td>Last Name:</td>
            <td><%= @user.last_name%></td>
          </tr>
          <tr>
            <td>Email Address:</td>
            <td><%= @user.email%></td>
          </tr>
        </tbody>
      </table>
    </div>
  </div>
  <div class="col-md-6">
    <div class="row">
      <h1>Your Events.</h1>
    </div>
    <div class="row">
      <p>
        All the events will show up here!
      </p>
    </div>
  </div>
</div>
